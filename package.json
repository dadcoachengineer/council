{
  "name": "council",
  "version": "0.1.0",
  "description": "Multi-agent MCP orchestrator modeled as a corporate governance board",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "dev:web": "vite dev src/web",
    "build": "tsup && vite build",
    "build:server": "tsup",
    "build:web": "vite build",
    "start": "node dist/server/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "better-sqlite3": "^11.7.0",
    "drizzle-orm": "^0.38.0",
    "express": "^5.0.1",
    "js-yaml": "^4.1.0",
    "nanoid": "^5.0.9",
    "ws": "^8.18.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.9.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.0",
    "@types/ws": "^8.5.13",
    "drizzle-kit": "^0.30.0",
    "eslint": "^9.16.0",
    "preact": "^10.25.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^2.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  }
}
